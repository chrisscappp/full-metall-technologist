      "assets/crimping_report_template_2.xlsx",
      "assets/crimping_report_template_3.xlsx",
      "assets/crimping_report_template_4.xlsx",
      "assets/crimping_report_template_5.xlsx",
      "assets/crimping_report_template_6.xlsx",
      "assets/crimping_report_template_7.xlsx",
      "assets/crimping_report_template_8.xlsx",
      "assets/crimping_report_template_9.xlsx",
      "assets/drowing_report_template_1.xlsx",
      "assets/drowing_report_template_2.xlsx",
      "assets/drowing_report_template_3.xlsx",
      "assets/drowing_report_template_4.xlsx",
      "assets/drowing_report_template_5.xlsx",
      "assets/drowing_report_template_6.xlsx",
      "assets/drowing_report_template_7.xlsx",
      "assets/drowing_report_template_8.xlsx",
      "assets/drowing_report_template_9.xlsx",
      "assets/felling_report_template.xlsx",
      "assets/Краткая_документация_по_процессу_создания_переменных_в_Компас_3Д.pdf"
    ]
  }
}
